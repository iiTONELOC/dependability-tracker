{
  "name": "dependability-tracker",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "lint": "next lint",
    "ts": "node -r ts-node/register",
    "update-packages": "npm-check -u",
    "clean": "node ./scripts/clean.mjs",
    "update": "node ./scripts/update.mjs",
    "removeTel": "npx next telemetry disable",
    "seed": "ts-node ./src/scripts/seedDb.ts",
    "uninstall": "node ./scripts/uninstall.mjs",
    "build": "next build && tsc -p tsconfig.node.json",
    "createEnv": "npm run ts -- ./src/scripts/createEnv.ts",
    "seed-callout": "ts-node ./src/lib/db/seeds/callouts.ts",
    "install-service": "node ./scripts/install-as-service.mjs",
    "genTLS": "npm run ts -- ./src/scripts/generateTLSCerts.ts",
    "sendInvite": "npm run ts -- ./src/scripts/loginCredInvite.ts",
    "seed-callouts-365": "ts-node ./src/lib/db/seeds/callouts.ts 365",
    "test": "node --env-file='.env.test' ./node_modules/.bin/jest --runInBand --detectOpenHandles",
    "start": "concurrently \"npm run ts ./src/scripts/removeExpired.ts\" \"cross-env NODE_ENV=production node ./dist/server/index.js\"",
    "dev": "concurrently \"npm run ts ./src/scripts/removeExpired.ts\" \" cross-env PORT=5000 nodemon --watch src --ext ts --exec ts-node ./src/server/index.ts\""
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.spec/**"
    ]
  },
  "dependencies": {
    "@react-pdf/renderer": "^3.4.4",
    "bcrypt": "^5.1.1",
    "chart.js": "^4.4.3",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mariadb": "^3.3.1",
    "mysql2": "^3.10.2",
    "next": "14.2.4",
    "nodemailer": "^6.9.14",
    "pg": "^8.12.0",
    "pg-hstore": "^2.3.4",
    "react": "^18",
    "react-dom": "^18",
    "react-test-renderer": "^18.3.1",
    "sequelize": "^6.37.3",
    "sharp": "^0.33.4"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.14.9",
    "@types/nodemailer": "^6.4.15",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/react-test-renderer": "^18.3.0",
    "eslint": "^8",
    "eslint-config-next": "14.2.4",
    "jest": "^29.7.0",
    "nodemon": "^3.1.4",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.1.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  }
}
